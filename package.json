  "type": "module",
  "scripts": {
    "commit": "commit",
    "prepare": "husky install",
    "serve": "node scripts/serve.js",
    "demo:build": "node scripts/demoBuild.js",
    "compile": "lerna run compile --parallel",
    "pretypecheck": "npm run compile",
    "typecheck": "vue-tsc --noEmit",
    "typecheck:only": "vue-tsc --noEmit",
    "prebuild": "npm run typecheck",
    "build": "node scripts/build.js",
    "test:unit": "vitest --config=vitest.config.ts",
    "test:unit:ci": "vitest run --config=vitest.config.js",
    "test:unit:u": "vitest -u --config=vitest.config.js",
    "test:e2e": "node scripts/e2e.js wdio.lambda.conf.cjs",
    "test:e2e:local": "node scripts/e2e.js wdio.local.conf.cjs",
    "lint": "eslint --fix --ext .js,.vue,.ts .",
    "custom:publish": "lerna publish -m \"chore: publish %s\" --yes --no-push --no-verify-access --force-publish=*",
    "docs:info": "vuepress info",
    "docs:build": "vuepress build docs",
    "docs:dev": "vuepress dev docs",
    "generate:icons": "node packages/@featherds/icon/scripts/createIcons.js",
    "new": "hygen new",
    "test": "npm run test:unit:ci",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
